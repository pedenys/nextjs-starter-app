{
  "name": "my-app",
  "private": true,
  "author": "pedenys",
  "license": "ISC",
  "keywords": [
    "nextjs",
    "typescript",
    "starter"
  ],
  "scripts": {
    "build": "next build",
    "deploy": "next build && next export",
    "dev": "next dev",
    "lint": "next lint",
    "precommit": "npm run lint && npm run prettier && npm run ts-check",
    "prepare": "husky install",
    "prettier": "prettier --config .prettierrc './**/*.{ts,tsx,json}' --write",
    "start": "next start",
    "ts-check": "tsc --noEmit --pretty"
  },
  "lint-staged": {
    "**/*": "npm run precommit"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "next": "12.0.4",
    "next-pwa": "^5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "prettier": "2.4.1",
    "typescript": "4.5.2"
  }
}
