{
  "name": "nextjs-starter",
  "version": "0.0.1",
  "description": "NextJS with TypeScript + emotion starter app",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "deploy": "next build && next export",
    "dev": "next",
    "prettier-format": "prettier --config .prettierrc './src/**/*.{ts,tsx,json}' --write",
    "start": "next start",
    "ts-check": "tsc --noEmit --pretty"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "emotion"
  ],
  "author": "pedenys",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "husky": "^4.2.5",
    "next": "^11.1.0",
    "next-offline": "^5.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.21",
    "@types/react": "^16.9.49",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run ts-check"
    }
  }
}
